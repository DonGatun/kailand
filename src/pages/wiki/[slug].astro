---
import Layout from '../../layouts/Contact.astro';
import NavBar from '../../components/NavBar.astro';
import Wiki from '../../components/Wiki.jsx';

export function getStaticPaths() {
    return [
        { params: { slug: 'inicio' } },
        { params: { slug: 'mods' } },
        { params: { slug: 'guia' } },
        { params: { slug: 'juego' } },
        { params: { slug: 'launcher' } },
        { params: { slug: 'comunicados' } },
    ]
}

const { slug } = Astro.params;
---

<Layout url='https://www.kailand.es/wiki' title="Kailand: Wiki" description="Wiki de Kailand">
    <NavBar />
    <main>
        <div class="h-10"></div>
        <Wiki client:load slug={slug} />
        <div class="h-10"></div>
    </main>
</Layout>